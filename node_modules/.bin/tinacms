#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/michelemarcucci/Desktop/blog-template/node_modules/.pnpm/@tinacms+cli@1.10.1_@codemirror+language@6.0.0_@types+hoist-non-react-statics@3.3.6_@ty_ff30ea2128462cd3e1ded67f1294d57f/node_modules/@tinacms/cli/bin/node_modules:/Users/michelemarcucci/Desktop/blog-template/node_modules/.pnpm/@tinacms+cli@1.10.1_@codemirror+language@6.0.0_@types+hoist-non-react-statics@3.3.6_@ty_ff30ea2128462cd3e1ded67f1294d57f/node_modules/@tinacms/cli/node_modules:/Users/michelemarcucci/Desktop/blog-template/node_modules/.pnpm/@tinacms+cli@1.10.1_@codemirror+language@6.0.0_@types+hoist-non-react-statics@3.3.6_@ty_ff30ea2128462cd3e1ded67f1294d57f/node_modules/@tinacms/node_modules:/Users/michelemarcucci/Desktop/blog-template/node_modules/.pnpm/@tinacms+cli@1.10.1_@codemirror+language@6.0.0_@types+hoist-non-react-statics@3.3.6_@ty_ff30ea2128462cd3e1ded67f1294d57f/node_modules:/Users/michelemarcucci/Desktop/blog-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/michelemarcucci/Desktop/blog-template/node_modules/.pnpm/@tinacms+cli@1.10.1_@codemirror+language@6.0.0_@types+hoist-non-react-statics@3.3.6_@ty_ff30ea2128462cd3e1ded67f1294d57f/node_modules/@tinacms/cli/bin/node_modules:/Users/michelemarcucci/Desktop/blog-template/node_modules/.pnpm/@tinacms+cli@1.10.1_@codemirror+language@6.0.0_@types+hoist-non-react-statics@3.3.6_@ty_ff30ea2128462cd3e1ded67f1294d57f/node_modules/@tinacms/cli/node_modules:/Users/michelemarcucci/Desktop/blog-template/node_modules/.pnpm/@tinacms+cli@1.10.1_@codemirror+language@6.0.0_@types+hoist-non-react-statics@3.3.6_@ty_ff30ea2128462cd3e1ded67f1294d57f/node_modules/@tinacms/node_modules:/Users/michelemarcucci/Desktop/blog-template/node_modules/.pnpm/@tinacms+cli@1.10.1_@codemirror+language@6.0.0_@types+hoist-non-react-statics@3.3.6_@ty_ff30ea2128462cd3e1ded67f1294d57f/node_modules:/Users/michelemarcucci/Desktop/blog-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@tinacms/cli/bin/tinacms" "$@"
else
  exec node  "$basedir/../@tinacms/cli/bin/tinacms" "$@"
fi
